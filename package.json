{
  "name": "ib-analyzer",
  "version": "0.3.2",
  "author": "fs-public",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "watch": "tsc -p . --watch",
    "build": "tsc -p .",
    "tsc": "tsc -p . --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:coverage:show": "jest --coverage && start coverage/lcov-report/index.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "copy_example:linux": "cp example/config/ src -r",
    "copy_example:windows": "XCOPY example\\config src\\config /I /Y",
    "premerge": "npm install && npm run format:check && npm run lint && npm run test && npm run tsc && npm run build",
    "start": "node --es-module-specifier-resolution=node --require=./suppress-experimental.cjs ."
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "csv": "^6.3.6",
    "dotenv": "^16.3.1",
    "handlebars": "^4.7.8",
    "html-pdf": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/html-pdf": "^3.0.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
