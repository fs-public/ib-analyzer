{
    "name": "ib-analyzer",
    "version": "0.3.1",
    "author": "fs-public",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "type": "module",
    "scripts": {
        "watch": "tsc -p . --watch",
        "build": "tsc -p .",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "test:coverage:show": "jest --coverage && start coverage/lcov-report/index.html",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format:write": "prettier . --write",
        "format:check": "prettier . --check",
        "copy_example:linux": "cp example/config/ src -r",
        "copy_example:windows": "XCOPY example\\config src\\config /I /Y",
        "premerge": "npm install && npm run format:check && npm run lint && npm run build",
        "start": "node --es-module-specifier-resolution=node --require=./suppress-experimental.cjs ."
    },
    "dependencies": {
        "ajv": "^8.12.0",
        "csv": "^6.3.1",
        "dotenv": "^16.0.3",
        "moment": "^2.29.4"
    },
    "devDependencies": {
        "@types/jest": "^29.5.1",
        "@types/node": "^20.2.4",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "eslint": "^8.41.0",
        "eslint-plugin-import": "^2.27.5",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "tslib": "^2.5.2",
        "typescript": "^5.0.4"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
